	.data
buf:	.space	64
format: .asciz "%d\n"
counter: .long 0
	.text
	.global	main
main:
	pushq	$0
	call 	inImage
inImage:
	movq	$buf, %rdi
	movq	$5, %rsi
	movq	stdin, %rdx
	call	fgets
getInt:
	movq	$counter,%rax
	pushq	%rax
	call	checkifInt
checkifInt:
	popq	%rsi
	movq	$buf,%rdi
	movq	(%rdi,%rsi,4),%rdi
	cmpq	$32, %rdi
	je		continueloop
	cmpq	$43, %rdi
	je		continueloop
	cmpq	$45, %rdi
	je		continueloop
	cmpq	$48, %rdi
	jle		checkifnumber
	call 	endloop
checkifnumber:
	cmpq	$57, %rdi
	jle		continueloop
continueloop:
	pushq	%rdi
	incq	%rax
	pushq	%rax
	call	checkifInt
endloop:

